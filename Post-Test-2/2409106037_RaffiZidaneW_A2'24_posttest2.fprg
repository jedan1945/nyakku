    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nim, harga" type="Integer" array="False" size=""/>
            <declare name="diskonMawar, hargamawar, diskonSania, hargaSania, diskonMaknyus, hargaMaknyus" type="Real" array="False" size=""/>
            <declare name="nama" type="String" array="False" size=""/>
            <output expression="&quot;mau beli beras?isi ini dulu boss&quot;" newline="True"/>
            <output expression="&quot;masukan nama untuk membeli beras&quot;" newline="True"/>
